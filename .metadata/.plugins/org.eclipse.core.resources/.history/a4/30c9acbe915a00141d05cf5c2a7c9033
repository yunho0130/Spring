<!-- fileList JavaScript -->
<script>
    var resultDiv = document.getElementById("fileList");
    var xhr = new XMLHttpRequest();
    console.log(xhr);
    xhr.open("get", "/140929_mp3_Prof/list", true);
    xhr.send(null);
    xhr.onreadystatechange = function(){
        if(xhr.readyState==4 && xhr.status == 200){
            var fileList = xhr.responseText;
            resultDiv.innerHTML = fileList;
            
        }
    }

</script>

<!-- fileinfo JavaScript -->
<script>
 var resultDiv2 = document.getElementById("fileinfo");
    var xhr2 = new XMLHttpRequest();
    console.log(xhr2);
    xhr2.open("get", "/140929_mp3_Prof/info", true);
    xhr2.send(null);
    xhr2.onreadystatechange = function(){
        if(xhr2.readyState==4 && xhr2.status == 200){
            var fileList2 = xhr2.responseText;
            resultDiv2.innerHTML = fileList2;
            
        }
    }
</script>
